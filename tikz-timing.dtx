\def\tikztiming@handletext@@@#1#2{%
  \expandafter\lowercase\expandafter{%
    \expandafter\def\expandafter\@tempa
    \expandafter{\tikztiming@currentchar}%
  }%
  \pgfkeysifdefined{/tikz/timing/\@tempa/text/.@cmd}%
  {%
